<script>
	require(['jquery', 'underscore', 'bookmark'], function($, _, Bookmark) {
		$('.torrent-item').each(function(){
			new Bookmark(this, {
				success: function(subscribed) {
					if(subscribed) {
						$(this).removeClass('button-primary').addClass('button-white').text('Удалить из закладок');
					} else {
						$(this).addClass('button-primary').removeClass('button-white').text('Добавить в закладки');
					}
				}
			});
		});

		$('.js-ajax-toggle').on('click', function(e){
			e.preventDefault();

			var $this = $(this);
			var $block = $this.closest('.spoiler');
			var $content = $block.children('.spoiler-content');
			var $list = $content.find('ol');

			var $loader = $block.find('.loader');
			var loaded = $this.data('loaded');
			var action = $this.data('action');

			if(loaded) {
				$content.toggle();
				return;
			}

			var template = _.template(document.getElementById(action + 'Template').innerHTML);

			$.ajax({
				url: '/torrent/65966cf587060e43aa1f3366/' + action + '/',
				success: function(data) {
					if(!data.s || !data.d) return;
					$list.html(template({data: data.d}));
					$content.show();
				},
				beforeSend: function() {
					$loader.removeClass('hidden');
				},
				complete: function(){
					$loader.addClass('hidden');
					$this.data('loaded', true);
				}
			});
		});
	});
</script>

